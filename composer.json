{
  "name": "lumen-cool/sdk",
  "description": "PHP client for uploading files to Lumen Files drives.",
  "type": "library",
  "license": "AGPL-3.0-or-later",
  "require": {
    "php": "^8.2",
    "ext-ctype": "*",
    "ext-fileinfo": "*",
    "ext-hash": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-sodium": "*",
    "guzzlehttp/guzzle": "^7.8",
    "nyra/bip39": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Lumen\\Sdk\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  }
}
